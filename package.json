{
  "name": "swd-eslint-config",
  "version": "2.1.6",
  "description": "Custom ESLint config",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastianwd/eslint-config.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "build": "tsup src",
    "ncu": "ncu -i",
    "release": "changeset",
    "release:publish": "pnpm build && changeset publish --provenance --access public"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "config",
    "react",
    "typescript",
    "javascript"
  ],
  "author": "Sebastian L",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "24.2.1",
    "eslint": "9.33.0",
    "npm-check-updates": "18.0.2",
    "prettier": "3.6.2",
    "tsup": "^8.5.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@eslint/compat": "1.3.2",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.33.0",
    "@next/eslint-plugin-next": "15.4.6",
    "@tanstack/eslint-plugin-query": "5.83.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-better-tailwindcss": "3.7.4",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "typescript-eslint": "8.39.0"
  },
  "peerDependencies": {
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "overrides": {
      "eslint-plugin-react-hooks": "^5.2.0"
    }
  },
  "packageManager": "pnpm@10.14.0"
}
