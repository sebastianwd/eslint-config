{
  "name": "swd-eslint-config",
  "version": "1.5.0",
  "description": "Custom ESLint config",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastianwd/eslint-config.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "build": "tsup src",
    "ncu": "ncu -i",
    "release": "changeset",
    "release:publish": "pnpm build && changeset publish --provenance --access public"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "config",
    "react",
    "typescript",
    "javascript"
  ],
  "author": "Sebastian L",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.3",
    "@types/node": "22.15.17",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "eslint": "^9.26.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "eslint-plugin-tailwindcss": "npm:@hyoban/eslint-plugin-tailwindcss@4.0.0-alpha.12",
    "@next/eslint-plugin-next": "^15.3.2",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "typescript-eslint": "^8.32.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "overrides": {
      "eslint-plugin-react-hooks": "^5.2.0"
    }
  },
  "packageManager": "pnpm@10.10.0"
}
